{
  "name": "sparxsolver",
  "version": "1.1",
  "description": "Fully native SparxMaths solver.",
  "main": "main.js",
  "scripts": {
      "start": "electron .",
      "package-win": "electron-packager . --platform=win32 --arch=x64 --out=release-builds --overwrite --icon=logo.ico",
      "package-mac-intel": "electron-packager . --platform=darwin --arch=x64 --out=release-builds --overwrite --icon=logo.icns",
      "package-mac-arm": "electron-packager . --platform=darwin --arch=arm64 --out=release-builds --overwrite --icon=logo.icns"
  },
  "devDependencies": {
      "electron": "^25.3.0",
      "electron-packager": "^16.0.0"
  },
  "dependencies": {
      "discord-webhook-node": "^1.1.8"
  }
}
